{
    "installCommad": "npm install express cors mongoose dotenv",
    "runCommand": "nodemon server.js",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/" }
    ]
}
